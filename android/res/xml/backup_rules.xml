<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
 <cloud-backup>
   <exclude domain="root" path="./"/>
   <include domain="root" path="./logs"/>
   <exclude domain="root" path="./cache"/>
   <include domain="root" path="Default.tsk"/>
   <include domain="root" path="default.prf"/>
